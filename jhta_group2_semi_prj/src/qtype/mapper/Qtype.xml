<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="qtype">

<typeAlias alias="qtype" type="pro.qtype.vo.Qtype"/>
	
<select id="getQtypeNoByQtype" resultClass="qtype" parameterClass="string">
	select qtype_no
	from qtype
	where qtype = #value#
</select>

</sqlMap>